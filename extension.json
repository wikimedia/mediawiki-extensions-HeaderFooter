		"remoteExtPath": "HeaderFooter/modules"
	},
	"ResourceModules": {
		"ext.headerfooter.dynamicload": {
			"scripts": [
				"dynamicload.js"
			]
		}
	},
	"config_prefix": "egHeaderFooter",
	"config": {
		"EnableAsyncHeader": {
			"value": false
