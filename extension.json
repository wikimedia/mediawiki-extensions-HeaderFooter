{
	"name": "HeaderFooter",
	"version": "4.0.0",
	"author": [
		"Jean-Lou Dupont",
		"James Montalvo",
		"Douglas Mason",
		"[https://www.hallowelt.com Hallo Welt! GmbH]"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:HeaderFooter",
	"descriptionmsg": "headerfooter-desc",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\HeaderFooter\\": "src/"
	},
	"AutoloadClasses": {
		"ApiGetHeaderFooter": "includes/api/ApiGetHeaderFooter.php"
	},
	"APIModules": {
		"getheaderfooter": "ApiGetHeaderFooter"
	},
	"HookHandlers": {
		"addHeaderFooter": {
			"class": "MediaWiki\\Extension\\HeaderFooter\\HookHandler\\AddHeaderFooter"
		},
		"addMagicWords": {
			"class": "MediaWiki\\Extension\\HeaderFooter\\HookHandler\\AddMagicWords"
		},
		"addConfigVars": {
			"class": "MediaWiki\\Extension\\HeaderFooter\\HookHandler\\AddConfigVars"
		}
	},
	"Hooks": {
		"ContentAlterParserOutput": "addHeaderFooter",
		"GetDoubleUnderscoreIDs": "addMagicWords",
		"ResourceLoaderGetConfigVars": "addConfigVars"
	},
	"MessagesDirs": {
		"HeaderFooter": [
			"i18n",
			"i18n/api"
		]
	},
	"ExtensionMessagesFiles": {
		"HeaderFooterMagic": "languages/HeaderFooter.i18n.magic.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "HeaderFooter/modules"
	},
	"ResourceModules": {
		"ext.headerfooter.dynamicload": {
			"scripts": [
				"dynamicload.js"
			]
		}
	},
	"config_prefix": "egHeaderFooter",
	"config": {
		"EnableAsyncHeader": {
			"value": false
		},
		"EnableAsyncFooter": {
			"value": false
		}
	},
	"manifest_version": 2
}
